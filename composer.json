{
    "name": "phpbench/phpbench",
    "description": "PHP Benchmarking Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "php": "^5.4|^7.0",
        "phpbench/dom": "^0.1",
        "symfony/console": "^2.4|^3.0",
        "symfony/finder": "^2.4|^3.0",
        "symfony/process": "^2.1|^3.0",
        "symfony/filesystem": "^2.4|^3.0",
        "symfony/debug": "^2.4|^3.0",
        "phpbench/tabular": "dev-master",
        "seld/jsonlint": "^1.0",
        "justinrainbow/json-schema": "^1.0",
        "doctrine/annotations": "^1.2.7",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-reflection": "*",
        "ext-spl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "PhpBench\\": "lib/",
            "PhpBench\\Extensions\\XDebug\\": "extensions/xdebug/lib/"
        }
    },
    "suggests": {
        "ext-xdebug": "For XDebug profiling extension."
    },
    "autoload-dev": {
        "psr-4": {
            "PhpBench\\Tests\\": "tests/",
            "PhpBench\\Benchmarks\\": "benchmarks/",
            "PhpBench\\Extensions\\XDebug\\Tests\\": "extensions/xdebug/tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": ["bin/phpbench"]
}
